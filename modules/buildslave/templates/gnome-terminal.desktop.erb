[Desktop Entry]
Type=Application
# use xterm instead of gnome-terminal, since gnome-terminal isn't installed and doesn't
# play nice with nvidia
Exec=xterm -e '<%= scope.lookupvar('::packages::mozilla::python27::python') %> /usr/local/bin/runslave.py'
Hidden=false
X-GNOME-Autostart-enabled=true
Name=Talos slave
Comment=Start runslave in a terminal session
StartupNotify=false
Terminal=false
Type=Application
