#Define Plugins
LoadPlugin df
<Plugin "df">
  FSType "ext3"
  IgnoreSelected true
# Since 4.9
  ReportReserved false
  ReportInodes false
</Plugin>

LoadPlugin disk
<Plugin "disk">
 Disk "/sd/"
 IgnoreSelected false
</Plugin>

LoadPlugin interface
<Plugin "interface">
 Interface "lo"
 IgnoreSelected true
</Plugin>

LoadPlugin vmem
<Plugin "vmem">
 Verbose flase
</Plugin>

#No extra args needed
LoadPlugin load
LoadPlugin memory
LoadPlugin swap
LoadPlugin uptime
LoadPlugin cpu
